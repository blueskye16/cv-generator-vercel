@media print {
  @page {
    size: A4;
    margin: 0mm !important;
  }

  html,
  body {
    margin: 0 !important;
    padding: 0 !important;
    width: 210mm;
    height: 297mm;
    background: white;
  }

  * {
    -webkit-print-color-adjust: exact !important;
    print-color-adjust: exact !important;
  }

  body > *:not(#cv-paper) {
    display: none;
  }

  #cv-paper {
    width: 210mm;
    min-height: 297mm;
    height: auto;
    padding: 20mm;
    /* padding: 12mm 8mm; */
    box-sizing: border-box;
  }
  
  /* #cv-paper {
    width: 210mm;
    height: 297mm;
    padding: 12mm 12mm;
    box-sizing: border-box;

    font-size: 9.5pt;
    line-height: 1.1;

    font-family: "Inter", "Arial", sans-serif;
  } */

  #cv-paper ul,
  #cv-paper ol {
    margin: 0;
    padding-left: 12px;
  }

  #cv-paper li {
    margin: 0;
    padding: 0;
  }

  #cv-paper p {
    margin: 0;
    padding: 0;
    line-height: 1.1;
  }

  #cv-paper ul li p {
    display: inline;
  }
}
